@model IEnumerable<Walnut.Entities.JobVacancySkillSet>
@using Walnut.Extensions
@using Walnut.Models.CustomHTMLHelpers;
@{
    ViewBag.Title = "Index";
}

<h2>Job Vacancy and Skill Set UNION</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.JobVacancyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SkillSetId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @*@Html.DisplayFor(modelItem => item.JobVacancyId)*@
            @item.GetJobVacancyDescription()
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.SkillSetId)*@
            @item.GetSkillSetDescription()
        </td>
        <td>
            @Html.CRUD_ActionLink("Delete", item.GetType().Name, "Delete", item.Id, true)
        </td>
    </tr>
}

</table>
@Scripts.Render("~/bundles/WalnutUIList")